{
  "name": "Muniversal-Router",
  "description": "A more universal router than Uniswap's Universal Router",
  "license": "GPL-3.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.1",
  "keywords": [
    "uniswap",
    "router",
    "universal router",
    "swap router"
  ],
  "scripts": {
    "build": "forge build",
    "size": "forge build --sizes",
    "test": "forge test",
    "snapshot": "forge snapshot",
    "snapshot:diff": "forge snapshot --diff",
    "prettier": "prettier --check {script,src,test}/**/*.sol",
    "prettier:fix": "prettier -w {script,src,test}/**/*.sol"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Aureliano90/Muniversal-Router"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@uniswap/universal-router": "^1.5.0",
    "@uniswap/v2-core": "1.0.1",
    "@uniswap/v3-core": "1.0.1",
    "@openzeppelin/contracts": "^4.9.3"
  },
  "devDependencies": {
    "prettier": "^3.0.3",
    "prettier-plugin-solidity": "^1.1.3"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-solidity"
    ],
    "endOfLine": "lf",
    "printWidth": 120
  }
}
